{
  "name": "kickerino",
  "version": "0.1.33",
  "description": "A chat application",
  "main": "./build/electron/electron.js",
  "homepage": "https://github.com/vicodinee/kickerino",
  "author": {
    "name": "vicodinee"
  },
  "build": {
    "asar": true,
    "productName": "kickerino",
    "files": [
      "build/**/*",
      "!node_modules"
    ],
    "appId": "com.kickerino.app",
    "mac": {
      "category": "public.app-category.social-networking",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
    },
    "win": {
      "target": "nsis",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Network"
    },
    "publish": {
      "provider": "github",
      "repo": "kickerino",
      "owner": "vicodinee",
      "releaseType": "release"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:local": "vite build && npm run app:dist",
    "dev": "vite",
    "test:electron": "jest --config=jest.electron.config.js",
    "test:react": "jest --config=jest.react.config.js",
    "lint:fix": "npx eslint src --fix",
    "prettier:fix": "npx prettier src --check --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "npx tsc --noEmit && npm run test:electron && npm run test:react",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/rollup-plugin-postcss": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "prettier": "3.1.0",
    "storybook": "^7.6.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.4",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.3",
    "@heroicons/react": "^2.0.18",
    "@tabler/icons-react": "^2.44.0",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-virtual": "^3.0.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "debounce": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "zustand": "^4.4.6"
  }
}
